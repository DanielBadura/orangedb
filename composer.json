{
    "name": "davidbadura/orangedb",
    "description": "Readonly Filesystem Database | Object-File Mapping",
    "keywords": ["database", "filesystem", "yaml", "json", "annotation"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name" : "David Badura",
        "email" : "d.badura@gmx.de"
    }],
    "require": {
        "php": ">=7.1",
        "symfony/yaml": "^3.0",
        "symfony/finder": "^3.0",
        "symfony/event-dispatcher": "^3.0",
        "doctrine/instantiator": "^1.0",
        "webmozart/json": "^1.2",
        "webmozart/path-util": "^2.3",
        "jms/metadata": "^1.5",
        "doctrine/annotations": "^1.2",
        "timetoogo/pinq": "^3.3",
        "psr/cache": "^1.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^3.0",
        "phpunit/phpunit": "^5.0",
        "symfony/cache": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "DavidBadura\\OrangeDb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DavidBadura\\OrangeDb\\Test\\": "tests"
        }
    }
}
